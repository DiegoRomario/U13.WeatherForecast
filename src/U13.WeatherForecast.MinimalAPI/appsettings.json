{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HttpClient": {
    "GeocodingBase": "https://geocoding.geo.census.gov/",
    "GeocodingByAddress": "geocoder/locations/onelineaddress?address={0}&benchmark=2020&format=json",
    "WeatherBase": "https://api.weather.gov/",
    "GridPointsByCoordinates": "points/{1},{0}",
    "ForecastByGrid": "gridpoints/{0}/{1},{2}/forecast"
  },
  "AllowedHosts": "*"
}
